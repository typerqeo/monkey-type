{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["off"],
    "no-empty": ["off"],
    "no-dupe-keys": ["off"],
    "no-func-assign": ["off"],
    "no-useless-catch": ["off"],
    "no-dupe-else-if": ["off"],
    "no-redeclare": ["off"],
    "no-unexpected-multiline": ["off"]
  },
  "globals": {
    "$": "readonly",
    "jQuery": "readonly"
  }
}
